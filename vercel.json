{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "cadastro-de-links-programa-de-afiliados-temu-brasil.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/.*",
            "dest": "/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}